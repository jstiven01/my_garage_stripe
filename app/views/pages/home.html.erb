<h1>This is Home</h1>
<% if user_signed_in? %>
    <p>Welcome <%= current_user.first_name %> to my home</p><%= link_to 'Sign out', destroy_user_session_path, method: :delete %>
<% else %>
    <p>Please go to registration..</p><%= link_to 'Registration', new_user_registration_path %> or <%= link_to 'Sign in', user_session_path %>
<% end %>
